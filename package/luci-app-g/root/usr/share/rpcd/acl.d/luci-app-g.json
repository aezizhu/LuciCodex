{
	"luci-app-g": {
		"description": "Grant access to g assistant configuration and execution",
		"read": {
			"uci": [ "g" ],
			"file": {
				"/tmp/g.log": [ "read" ],
				"/tmp/g-metrics.json": [ "read" ],
				"/tmp/g-plan.json": [ "read" ]
			}
		},
		"write": {
			"uci": [ "g" ],
			"file": {
				"/var/lock/g.lock": [ "write" ]
			},
			"exec": {
				"/usr/bin/g": [ "*" ]
			}
		}
	}
}
